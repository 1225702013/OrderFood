<script>
    var appraise = $("#allAppraise")
    $(document).ready(function(){
        getAllAppraise(1,0)
        windowRecommend()
        $.fn.raty.defaults.path = "${pageContext.request.contextPath }/statics/img";
        $(".n-star-cnt").raty({ readOnly: true, score: 4.7 });
    });

    function getAllAppraise(pageIndex,appraiseType) {
        $.ajax({
            type: "GET",//请求类型
            url: "${pageContext.request.contextPath }/appraiselist/${window.id}/"+pageIndex+"/"+appraiseType,//请求的url
            dataType: "json",//ajax接口（请求url）返回的数据类型
            success: function (data) {
                if(appraiseType == 0){
                    appraise.removeClass('current');
                    $("#allAppraise").addClass('current');
                    appraise = $("#allAppraise");

                }
                if(appraiseType == 1){
                    appraise.removeClass('current');
                    $("#goodAppraise").addClass('current');
                    appraise = $("#goodAppraise");
                }
                if(appraiseType == 2){
                    appraise.removeClass('current');
                    $("#mediumAppraise").addClass('current');
                    appraise = $("#mediumAppraise");
                }
                if(appraiseType == 3){
                    appraise.removeClass('current');
                    $("#badAppraise").addClass('current');
                    appraise = $("#badAppraise");
                }
                writeAppraise(data);
                writerPageList(data,appraiseType)
            },
            error: function (data) {//当访问时候，404，500 等非200的错误状态码
                alert("加载餐厅失败！" + data);
            }
        })
    }
    function windowRecommend() {
        $.ajax({
            type: "GET",//请求类型
            url: "${pageContext.request.contextPath }/recommendlist/${window.id}",//请求的url
            dataType: "json",//ajax接口（请求url）返回的数据类型
            success: function (data) {
                weiteRecommendList(data);
            },
            error: function (data) {//当访问时候，404，500 等非200的错误状态码
                alert("加载窗口失败！" + data);
            }
        })
    }
    
    function writeAppraise(data) {

        var appraises = "";
        for (var i = 0; i < data.list.length; i++) {
            appraises+=
                "<div class=\"p-comment-list clearfix\"><div class=\"author-info\"><div class=\"photo\"><img src=\"${pageContext.request.contextPath}/statics/img/studentImg.jpg\" alt=\"用户头像\"></div><div class=\"author\">"+data.list[i].studentid.slice(0,3)+"****"+data.list[i].studentid.slice(-3)+"</div></div><div class=\"comment-info\"><div class=\"author clearfix\"><div class=\"fl\"><span class=\"time\">2019-02-23 09:25:37 说：</span></div><div class=\"fr\"><div class=\"star ugc-star-wrap \">\n" +
                "<div class=\"star ugc-star-grade\" style=\"width:28px;\">\n" +
                "</div>\n" +
                "</div>\n" +
                "</div>\n" +
                "</div>\n" +
                " <p class=\"content\">"+data.list[i].info+"</p>\n" +
                " </div>\n" +
                " </div>"
        }
        $("#j-comment-list").html(appraises);
        
    }

    function writerPageList(data,appraiseType) {
        var pagelist = "";
        if(data.pageIndex > 1){
            pagelist += "<a href='javascript:;' onclick='getAllAppraise("+(data.pageIndex-1)+","+appraiseType+")' data-pagenum=\"1\" class=\"ui-pager-prev\">" +
                "                    <div class=\"ico-pre\"></div></a>\n";
        }
        for (var i = 0; i < data.pageSize; i++) {
            if(i == data.pageIndex-1){
                pagelist += "<span class='ui-pager-current' >"+(i+1)+"</span>"
            }else {
                pagelist += "<a href='javascript:;' onclick='getAllAppraise("+(i+1)+","+appraiseType+")' class='ui-pager-normal' >"+(i+1)+"</a>"
            }

        }
        if(data.pageIndex < data.pageSize){
            pagelist += "<a href='javascript:;' onclick='getAllAppraise("+(data.pageIndex+1)+","+appraiseType+")'  class='ui-pager-next'><div class='ico-next'></div></a>"
        }
        $("#pagelist").html(pagelist);

    }
    function weiteRecommendList(data) {
        var recommendList = "<h3 class=\"ugc-recommend-header\">相似窗口推荐推荐</h3>";
        for (var i = 0; i < data.length; i++) {

            recommendList+="            <a class=\"ugc-recommend-item\" href='${pageContext.request.contextPath}/windowInfo/'"+data[i].id+">\n" +
                "                <p class=\"ugc-recommend-item-line ugc-recommend-item-title\">"+data[i].windowname+"</p>\n" +
                "                <p class=\"ugc-recommend-item-line\">\n" +
                "<span class=\"n-star n-star-small\">\n" +
                "<span class=\"n-star-cnt\" style=\"width: 63.4116px;\"></span>\n" +
                "</span>\n" +
                "                </p>\n" +
                "                <p class=\"ugc-recommend-item-line color-gray-666\">\n" +
                "<span>\n" +
                "人均：￥13元</span>\n" +
                "                </p>\n" +
                "                <div class=\"ugc-recommend-item-img\" style=\"background: url('${pageContext.request.contextPath}/statics/img/default_winImg.jpg')/*tpa=https://gss0.baidu.com/-4o3dSag_xI4khGko9WTAnF6hhy/bainuo/crop%3D0%2C49%2C690%2C418%3Bw%3D230%3Bq%3D80/sign=cf3557a9968fa0ec6b883e4d1ba775da/242dd42a2834349baedefb1fceea15ce37d3be54.jpg*/ 50% 50% / cover no-repeat;\"></div>\n" +
                "            </a>";
        }
        $('.ugc-recommend').html(recommendList);
    }

</script>


